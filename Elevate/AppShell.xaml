<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Elevate.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Elevate"
    Shell.FlyoutBehavior="Locked"
    Title="Elevate">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="Home" />

    <ShellContent
        Title="TODO"
        ContentTemplate="{DataTemplate local:Todo}"
        Route="Todo" />

    <ShellContent
        Title="Mapping"
        ContentTemplate="{DataTemplate local:Mapping}"
        Route="Mapping" />

    <ShellContent
        Title="Sequence"
        ContentTemplate="{DataTemplate local:Sequence}"
        Route="Sequence" />

    <ShellContent
        Title="Week"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Analytics"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
